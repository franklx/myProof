<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/tab_logic"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageButton android:tag="drop"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/drop"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="free"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/free"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="substitute"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/subs"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="new"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/newv"
            android:onClick="onCommandClick"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageButton android:tag="display_premise"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/premise"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="display_statement"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/statement"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="display_term"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/term"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="display_variable"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/variable"
            android:onClick="onCommandClick"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageButton android:tag="sequent"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/sequent"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="display_sentence"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/sentence"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="display_constant"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/constant"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="comma"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/comma"
            android:onClick="onCommandClick"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageButton android:tag="or"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/or"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="and"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/and"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="not"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/not"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="then"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/then"
            android:onClick="onCommandClick"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageButton android:tag="exor"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/exor"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="forall"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/forall"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="exists"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:src="@drawable/exists"
            android:onClick="onCommandClick"/>
        <ImageButton android:tag="iff"
            android:layout_width="@dimen/button_width"
            android:layout_height="@dimen/button_height"
            android:layout_margin="0pt"
            android:src="@drawable/iff"
            android:onClick="onCommandClick"/>
    </LinearLayout>
</LinearLayout>